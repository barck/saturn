*
	min-width: 0;
	min-height: 0;
	box-sizing: border-box;

html
	box-sizing border-box
	background #fff 
	font 16px Roboto, sans-serif
	line-height 24px
	font-weight: 300
	color #fff
	overflow-x: hidden;


body
	overflow-x: hidden;
img
	width: 100%

.container
	width 960px
	padding 0 10px
	margin 0 auto
	@media (max-width 992px)
		width: 750px
	@media (max-width 765px)
		width: 100%
header
	//margin-bottom:100px
	width: 100%
	position: fixed
	z-index 30
	@media (max-width 992px)
		position: relative
	@media (max-width 765px)
		background: #000
.head-flex
	display flex
	justify-content center
	padding: 0 40px
	position: relative
	@media (max-width 1032px )
		justify-content: center
	@media (min-width 1450px)
		padding: 0 160px
	@media (min-width 1900px)
		padding: 0 350px
	@media (max-width 992px)
		padding: 0
	nav
		@media (max-width 992px)
			width: 100%

.start-block
	background url("../img/6.jpg") repeat fixed
	background-size cover
	color #fff
	padding-top: 20px
	text-align: center
	padding-bottom: 1px
	.container
		margin-top: 180px
		@media (max-width 765px)
			margin-top: 70px
	@media (max-width 992px)
		padding-top: 0
		background url("../img/6.jpg") no-repeat center center
		background-size cover
.block-2
	background #000 url("../img/5.jpg") repeat fixed
	background-size cover
	padding-bottom: 1px
	padding-top: 20px
.block-3
	background #000 url("../img/3.jpg") repeat fixed
	background-size cover
	padding-bottom: 30px
	padding-top: 20px
.block-4
	background #000 url("../img/1.jpg") repeat fixed
	background-size cover
	padding-bottom: 30px
	padding-top: 20px
.block-5
	background #000 url("../img/8.jpg") repeat fixed
	min-height 500px
	background-size cover
	padding-bottom: 30px
	padding-top: 20px
.block-6
	background #fff
	padding-top: 20px
	padding-bottom: 40px
.block-7
	background #000 url("../img/9.jpg") repeat fixed
	min-height 200px
	padding-top: 20px
	padding-bottom: 30px
	@media (max-width 765px)
		padding-bottom: 0
.main-footer
	display: flex
	flex-wrap: wrap
	justify-content: space-between
	min-height 88px
	background #151515
	padding-bottom: 1px
	align-items center
.user-block
	background #000 url("../img/1.jpg") repeat fixed
	background-size cover
	padding-bottom: 1px
	padding-top: 20px
	@media (max-width 992px)
		padding-top: 0

.similar-works
	background #000 url("../img/3.jpg") repeat fixed
	background-size cover
	padding-bottom: 1px
	padding-top: 20px
	padding-bottom: 30px
//ELEMENTS
.logo
	width: 150px
	background: #f9c438
	transform: skew(35deg, 0deg);
	min-height 80px
	border-radius 5px
	padding-top: 10px
	margin-right: 10px
	box-shadow: 0px 0px 20px 0px rgba(249,196,56,0.5);
	&:hover
		background #8b6500
	@media (max-width 1209px)
		display: none
	img
		width: 40%
		transform: skew(-35deg, 0deg);

.nav
	display: inline-block
	background #000
	transform: skew(35deg, 0deg);
	border-radius 5px
	margin: 0
	padding: 0
	list-style none
	text-align left
	padding-left: 20px
	font-size 14px
	box-shadow: 0px 0px 20px 0px rgba(255,255,255,0.5);
	@media (max-width 1130px)
		font-size 11px
	@media (max-width 992px)
		display: flex
		justify-content: space-between
		font-size 15px
		transform none
		border-radius 0
		padding-left: 0
	@media (max-width 765px)
		display: none
		box-shadow none
		position: absolute
		width: 100%
	li
		transform: skew(-35deg, 0deg)
		display: inline-block
		border-bottom 2px solid transparent
		padding-bottom: 0
		position: relative
		@media (max-width 992px)
			transform none
			border-left: 1px solid rgba(255, 255, 255, 0.15);
			&:hover
				border none
		@media (max-width 765px)
			display: block
			border-color #f9c438
		&:last-child
			position: relative
			padding-left: 10px
			vertical-align: top

			a
				padding-top: 10px
				padding-bottom: 0
			&:hover
				border-color transparent
		&:hover
			border-color #f9c438
		&:hover ul
			@media(min-width 768px)
				display: block
		ul
			display none
			position: absolute
			z-index 100
			background: #000
			border-radius: 5px
			top: 50px
			left: 0
			padding 0
			margin: 0
			li
				display: block
				transform: skew(0deg, 0deg)
				&:last-child
					padding-left: 0
					&:hover
						border-color: #f9c438;
					a
						padding 10px 20px
				a
					text-decoration: none
					color #fff
					display: block
					padding 10px 20px
					padding-bottom 18px
					transition 0.2s

	a.nav-items
		text-decoration: none
		color #fff
		display: block
		padding 20px 20px
		padding-bottom 18px
		transition 0.2s
		outline none
		border none
		@media (max-width 992px)
			padding: 20px 8px
			font-size 12px

.mobile-logo
	display: none
	@media (max-width 765px)
		display: block
		position: absolute
		color #fff
		font-size 32px
		line-height 42px
.toggle
	display: block

//HEADER END

