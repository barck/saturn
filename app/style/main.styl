//second block
.btn
	display: block
	outline none
	border none
	background: linear-gradient(to bottom, #f9c438 0%, #f5bf39 26%, #f3bd37 29%, #eeb738 50%, #ecb534 55%, #e1a932 100%)
	width: 100%
	min-height 40px
	line-height: 32px;
	font-weight: 300
	font-size: 24px;
	text-decoration: none;
	text-align: center;
	color: #293234;
	border-color: #fac53a;
	border-radius: 5px;
	box-shadow: 0px 0px 20px 0px rgba(249,196,56,0.5);
	&:hover
		background: linear-gradient(to bottom, #ffd97a 0%, #fcd877 18%, #fed470 30%, #facb63 61%, #f8c75e 68%, #f5c153 100%)
.areas
	display flex
	justify-content space-between
	flex-wrap wrap
