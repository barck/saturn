	.areas-item
		min-height 200px
		width: 20%
		@media (max-width 765px)
			width: 48%
			margin-bottom: 50px
	img
		width:100%
		margin-bottom: 40px
	p
		margin: 0
		margin-bottom: 40px
		padding: 10px
		text-align: center
		font-size 18px
		line-height 26px
		@media (max-width 992px)
			padding: 0

.areas-slider
	margin-bottom: 50px
	display block
	.areas-item
		display: inline-block
		min-height 200px
		width: 20%
	img
		width: 180px
	p
		margin: 0
		margin-bottom: 10px
		padding: 10px
		text-align: center
.cta
